/**index.wxss**/

.swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.swiper image {
  width: 100%;
}

.cates {
  display: flex;
}
.cates navigator {
  flex: 1;
  padding: 25rpx;
}
.cates image {
  width: 100%;
}

.floor_title {
  padding: 10rpx 0;
}
.floor_title image {
  width: 100%;
}
.floor_list {
  overflow: hidden;
}
.floor_list navigator {
  float: left;
  width: 33.33%;
}
.floor_list navigator:nth-last-child(-n+4) {
  height: 27.73vw;
  border-left: 10rpx solid #fff;
}
.floor_list navigator:nth-child(2), .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor_list image {
  width: 100%;
  height: 100%;
}
